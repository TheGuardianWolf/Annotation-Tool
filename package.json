{
  "name": "angular2-electron-starter",
  "version": "0.1.3",
  "repository": "none",
  "license": "MIT",
  "main": "",
  "scripts": {
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "build": "./node_modules/.bin/webpack --inline --colors --progress --display-error-details --display-cached",
    "build:prod": "./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-dedupe",
    "build:dist": "npm run build:prod && npm run electron",
    "start": "./node_modules/.bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3333  --content-base app",
    "electron": "npm run build && electron app",
    "package": "./node_modules/electron-packager/cli.js app App --platform=win32 --arch=all --out=dist --overwrite --version='1.2.5'",
    "pack-linux": "electron-packager . --asar --asar-unpack=protocol-link.html --overwrite --platform=linux --arch=x64 --prune=true --out=out",
    "test": "mocha tests"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "css-loader": "^0.23.1",
    "electron": "^1.2.5",
    "electron-packager": "^7.1.0",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "node-sass": "^3.8.0",
    "reflect-metadata": "^0.1.9",
    "sass-loader": "^3.2.2",
    "source-map-loader": "^0.1.5",
    "spectron": "^3.2.3",
    "style-loader": "^0.13.1",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4",
    "typings": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.2",
    "@angular/core": "^2.4.2",
    "@angular/forms": "^2.4.2",
    "@angular/http": "^2.4.2",
    "@angular/platform-browser": "^2.4.2",
    "@angular/platform-browser-dynamic": "^2.4.2",
    "@angular/platform-server": "^2.4.2",
    "@angular/router": "^3.4.2",
    "bootstrap-material-design": "^0.5.10",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.0",
    "jquery": "^3.1.1",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.4"
  }
}
